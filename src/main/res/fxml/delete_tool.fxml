<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="480.0" minWidth="640.0"
            prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.DeleteToolController">
    <children>
        <TextField fx:id="txtDelToolID" editable="false" layoutX="15.0" layoutY="55.0" prefHeight="25.0"
                   prefWidth="570.0" promptText="ID"/>
        <TextField fx:id="txtDelToolName" editable="false" layoutX="15.0" layoutY="96.0" prefHeight="25.0"
                   prefWidth="570.0" promptText="Name"/>
        <TextField fx:id="txtDelToolAddress" editable="false" layoutX="15.0" layoutY="145.0" prefHeight="25.0"
                   prefWidth="570.0" promptText="Address"/>
        <Button fx:id="btnDelToolBack" layoutX="15.0" layoutY="191.0" mnemonicParsing="false" onAction="#openMainMenu"
                text="Back"/>
        <Button fx:id="btnDelToolScan" layoutX="275.0" layoutY="191.0" mnemonicParsing="false" onAction="#scanTool"
                text="Scan"/>
        <Button fx:id="btnDelToolDelete" layoutX="535.0" layoutY="191.0" mnemonicParsing="false" onAction="#deleteTool"
                text="Delete"/>
        <Label layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="570.0" text="Delete Tool">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label fx:id="lblDelToolError" layoutX="15.0" layoutY="231.0" prefHeight="25.0" prefWidth="570.0"
               textFill="#ff0202"/>
        <Label fx:id="lblDelToolMsg" layoutX="15.0" layoutY="267.0" prefHeight="25.0" prefWidth="570.0"
               textFill="#03ff11"/>
    </children>
</AnchorPane>
