<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="564.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.AddToolController">
    <children>
        <TextField fx:id="txtTagID" editable="false" layoutX="12.0" layoutY="108.0" prefHeight="26.0" prefWidth="444.0" AnchorPane.leftAnchor="12.0" AnchorPane.topAnchor="108.0" />
        <Label fx:id="lblToolName" layoutX="471.0" layoutY="156.0" prefHeight="26.0" prefWidth="64.0" text="Tool Name" AnchorPane.leftAnchor="471.0" AnchorPane.rightAnchor="65.0" />
        <Label fx:id="lblAddress" layoutX="471.0" layoutY="199.0" prefHeight="26.0" prefWidth="51.0" text="Address" AnchorPane.leftAnchor="471.0" AnchorPane.rightAnchor="65.0" />

        <Label fx:id="lblTagID" layoutX="471.0" layoutY="108.0" prefHeight="26.0" prefWidth="17.0" text="ID" AnchorPane.leftAnchor="471.0" AnchorPane.rightAnchor="65.0" />
        <TextField fx:id="txtToolName" layoutX="12.0" layoutY="156.0" onKeyReleased="#searchOpenAddresses" prefHeight="26.0" prefWidth="444.0" promptText="Tool Name" AnchorPane.leftAnchor="12.0" />
      <Label fx:id="lblError" layoutX="17.0" layoutY="433.0" prefHeight="17.0" prefWidth="567.0" textFill="RED" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="17.0" AnchorPane.rightAnchor="16.0" />
