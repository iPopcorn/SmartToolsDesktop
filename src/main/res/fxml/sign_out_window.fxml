<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="CENTER" focusTraversable="true" hgap="10.0" maxHeight="200.0" maxWidth="400.0" minHeight="240.0"
          minWidth="400.0" prefHeight="210.0" prefWidth="400.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.102"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.SignOutController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="515.0" minWidth="10.0" prefWidth="281.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="49.33332824707031" minHeight="7.333332061767578" prefHeight="9.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="88.0" minHeight="10.0" prefHeight="88.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="Are You Sure you Want To Sign Out?" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets/>
            </GridPane.margin>
        </Label>
        <Button fx:id="btnNo" cancelButton="true" mnemonicParsing="false" onAction="#btnNoPressed" text="No"
                GridPane.halignment="CENTER" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets bottom="30.0" right="60.0"/>
            </GridPane.margin>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button fx:id="btnYes" mnemonicParsing="false" onAction="#btnYesPressed" text="Yes" GridPane.halignment="CENTER"
                GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets bottom="30.0" left="60.0"/>
            </GridPane.margin>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
    </children>
</GridPane>
