<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="480.0"
            minWidth="640.0" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="main.LoginController">
    <children>
        <TextField fx:id="txtUsername" layoutX="226.0" layoutY="115.0" promptText="Username"/>
        <PasswordField fx:id="txtPassword" layoutX="226.0" layoutY="155.0" promptText="Password"/>
        <Button fx:id="btnBack" layoutX="315.0" layoutY="200.0" mnemonicParsing="false" onAction="#goBack"
                text="Go Back"/>
        <Button fx:id="btnLogIn" layoutX="226.0" layoutY="200.0" mnemonicParsing="false" onAction="#submitLogin"
                text="Log In"/>
    </children>
</AnchorPane>
